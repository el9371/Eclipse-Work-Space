<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <AnchorPane fx:id="mainPage" prefHeight="400.0" prefWidth="600.0">
         <children>
            <ImageView fx:id="background" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../image/background.png" />
               </image>
            </ImageView>
            <TextArea fx:id="systemMessage" editable="false" layoutX="400.0" prefHeight="371.0" prefWidth="200.0" text="System :" />
            <Button fx:id="gameButton" layoutX="400.0" layoutY="375.0" mnemonicParsing="false" onAction="#gameButton" prefHeight="20.0" prefWidth="70.0" text="START" />
            <Button layoutX="474.0" layoutY="375.0" mnemonicParsing="false" onAction="#undo" text="UNDO" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
